CREATE TABLE eventlog
(
    id serial,
    signature varchar(64),
    events text,
    "timestamp" timestamp NOT NULL DEFAULT now(),
    PRIMARY KEY (id)
)

CREATE TABLE vote
(
	id serial,
	vote_id varchar(64),
	user_id varchar(100) NOT NULL,
	display_name varchar(100) NOT NULL,
	room_id varchar(100) NULL,
	title text,
	description text,
	status smallint DEFAULT '0',
	PRIMARY KEY(vote_id)
)

CREATE TABLE vote_contain
(
	id serial,
	vote_id varchar(64) REFERENCES vote (vote_id),
	candidates varchar(100),
	cand_images varchar(250) NULL,
	votes int,
	PRIMARY KEY(id)
)